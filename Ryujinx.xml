<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-nx-nro">
        <comment>Nintendo Switch homebrew executable</comment>
        <acronym>NRO</acronym>
        <icon name="Ryujinx"/>
        <glob pattern="*.nro"/>
        <magic><match value="NRO0" type="string" offset="16"/></magic>
    </mime-type>

    <mime-type type="application/x-nx-nso">
        <comment>Nintendo Switch homebrew executable</comment>
        <acronym>NSO</acronym>
        <icon name="Ryujinx"/>
        <glob pattern="*.nso"/>
        <magic><match value="NSO0" type="string" offset="0"/></magic>
    </mime-type>
    
    <mime-type type="application/x-nx-nsp">
        <comment>Nintendo Switch Package</comment>
        <acronym>NSP</acronym>
        <icon name="Ryujinx"/>
        <glob pattern="*.nsp"/>
        <magic><match value="PFS0" type="string" offset="0"/></magic>
    </mime-type>

    <mime-type type="application/x-nx-xci">
        <comment>Nintendo Switch Card Image</comment>
        <acronym>XCI</acronym>
        <icon name="Ryujinx"/>
        <glob pattern="*.xci"/>
        <magic><match value="HEAD" type="string" offset="4352"/></magic>
    </mime-type>
</mime-info>
